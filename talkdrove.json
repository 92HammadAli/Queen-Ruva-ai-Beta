{
    "bot-name": "Queen-Ruva-AI-Beta",
    "description": "Queen Ruva AI Beta - Advanced WhatsApp Bot by Iconic Tech",
    "logo": "https://raw.githubusercontent.com/iconic05/Queen-ruva-ai-beta/main/assets/logo.jpg",
    "keywords": ["bot", "ai", "whatsapp"],
    "documentation-link": "https://github.com/iconic05/Queen-ruva-ai-beta",
    "owner-verification": "iconictech@example.com",
  
    "env": {
      "SESSION_ID": {
        "description": "Put your SESSION_ID here or use creds.json for session management",
        "value":"",
        "required": false
      },
      "PREFIX": {
        "description": "Bot command prefix (e.g., . , ! @ etc)",
        "value":".",
        "required": true
      },
      "AUTO_READ_STATUS": {
        "description": "Bot will read all status (true/false)",
        "value":"true",
        "required": false
      },
      "MODE": {
        "description": "Bot mode (public/private/inbox/groups)",
        "value":"public",
        "required": true
      },
      "OWNER_NUMBER": {
        "description": "Owner number with country code (e.g., 1234567890)",
        "value":"",
        "required": true
      },
      "READ_CMD": {
        "description": "Bot will read commands (true/false)",
        "value":"false",
        "required": false
      },
      "AUTO_REPLY": {
        "description": "Auto-reply to common messages (true/false)",
        "value":"false",
        "required": false
      },
      "AUTO_REACT": {
        "description": "Auto-react to messages (true/false)",
        "value":"false",
        "required": false
      },
      "WELCOME": {
        "description": "Welcome new group members (true/false)",
        "value":"false",
        "required": false
      },
      "ANTI_BAD": {
        "description": "Delete messages with bad words (true/false)",
        "value":"false",
        "required": false
      },
      "ANTI_BOT": {
        "description": "Remove bots from groups (true/false)",
        "value":"false",
        "required": false
      },
      "ANTI_LINK": {
        "description": "Block links in groups (true/false)",
        "value":"false",
        "required": false
      },
      "ALWAYS_ONLINE": {
        "description": "Keep WhatsApp always online (true/false)",
        "value":"false",
        "required": false
      },
      "PRESENCE": {
        "description": "Presence status (online/offline/typing/recording/paused)",
        "value":"offline",
        "required": false
      },
      "COUNTRY_BLOCK": {
        "description": "Enable country blocking (true/false)",
        "value":"true",
        "required": false
      },
      "COUNTRY_BLOCK_CODE": {
        "description": "Country code to block (without +)",
        "value":"",
        "required": false
      },
      "PREMIUM_USERS": {
        "description": "Premium users numbers (comma separated)",
        "value":"",
        "required": false
      },
      "USE_CREDS_JSON": {
        "description": "Use creds.json for session (true/false)",
        "value":"true",
        "required": false
      }
    }
      }
